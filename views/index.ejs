<!doctype html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1>Your <%= title %></h1>
    <% messages.forEach(message =>{ %>
    <h3><%= message.user %></h3>
    <h4><%= message.text %></h4>
    <p><%= message.added %></p>
    <% })%>

    <button type="button" onclick="handleClick()">New Message</button>
  </body>
</html>
